{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "consoleAirlock": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/console-airlock",
      "sourceRoot": "projects/console-airlock/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/consoleAirlock",
            "index": "projects/console-airlock/src/index.html",
            "main": "projects/console-airlock/src/main.ts",
            "polyfills": "projects/console-airlock/src/polyfills.ts",
            "tsConfig": "projects/console-airlock/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/console-airlock/src/favicon.ico",
              "projects/console-airlock/src/assets"
            ],
            "styles": [
              "projects/console-airlock/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/console-airlock/src/environments/environment.ts",
                  "with": "projects/console-airlock/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "consoleAirlock:build:production"
            },
            "development": {
              "browserTarget": "consoleAirlock:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "consoleAirlock:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/console-airlock/src/test.ts",
            "polyfills": "projects/console-airlock/src/polyfills.ts",
            "tsConfig": "projects/console-airlock/tsconfig.spec.json",
            "karmaConfig": "projects/console-airlock/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/console-airlock/src/favicon.ico",
              "projects/console-airlock/src/assets"
            ],
            "styles": [
              "projects/console-airlock/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
